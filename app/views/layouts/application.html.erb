<!DOCTYPE html>
<html>
  <head>
    <title>Han</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% unless flash.empty? %>
    <ul>
    <% flash.each do |f| %>
      <li><%= f[0] %>: <%= f[1] %></li>
    <% end %>
    </ul>
    <% end %>
    <%= yield %>
  </body>
</html>
